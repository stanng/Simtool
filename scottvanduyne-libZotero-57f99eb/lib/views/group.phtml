<group 
    <?if($this->group->id):?>
        id="<?=$this->group->groupID?>"
    <?endif;?>
    owner="<?=$this->group->owner?>"
    type="<?=$this->group->type?>" 
    name="<?=$this->group->name?>" 
    <?if($this->group->libraryEnabled):?>
        libraryEnabled="<?=$this->group->libraryEnabled?>" 
        libraryEditing="<?=$this->group->libraryEditing?>" 
        libraryReading="<?=$this->group->libraryReading?>" 
        fileEditing="<?=$this->group->fileEditing?>"
    <?else:?>
        libraryEnabled="0"
    <?endif;?>
    hasImage="<?=$this->group->hasImage ? 1 : 0;?>"
    >
    <?if($this->group->description):?>
        <description><?=urlencode($this->group->description)?></description>
    <?endif;?>
    <?if($this->group->url):?>
        <url><?=$this->group->url?></url>
    <?endif;?>
</group>
